{
    "name": "colinodell/omnipay-bundle",
    "description": "Omnipay bundle for Symfony 5+",
    "keywords": [
        "omnipay",
        "bundle",
        "symfony",
        "payment"
    ],
    "homepage": "https://github.com/colinodell/omnipay-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "colinodell@gmail.com",
            "homepage": "https://www.colinodell.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "league/omnipay": "^3.0",
        "symfony/framework-bundle": "^5"
    },
    "require-dev": {
        "omnipay/dummy": "^3.0",
        "phpunit/phpunit": "^3.7|^4.8"
    },
    "autoload": {
        "psr-4": {
            "ColinODell\\OmnipayBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}